import pandas as pd
import numpy as np
# Read the edgelist from a CSV file where each row represents an interaction between two proteins
edgelist = pd.read_csv("4538.csv")

# Determine the list of unique proteins in the edgelist
nodes = list(set(edgelist['ORGLA02G0043600.1']).union(set(edgelist['ORGLA09G0110800.1'])))
num_nodes = len(nodes)

# Create an n x n matrix initialized to zero
adj_matrix2 = np.zeros((num_nodes, num_nodes))

# Iterate through the edgelist and update the matrix
for i, row in edgelist.iterrows():
    protein1, protein2 = row['ORGLA02G0043600.1'], row['ORGLA09G0110800.1']
    i, j = nodes.index(protein1), nodes.index(protein2)
    adj_matrix2[i][j] = 1
    adj_matrix2[j][i] = 1

print(adj_matrix2)
